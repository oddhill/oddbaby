; The name and description of the theme used on the admin/build/themes page.
name = Origin
description = This is the theme for this particular site
core = 7.x


; Stylesheets.
stylesheets[screen][] = css/reset.css
stylesheets[screen][] = css/messages.css
stylesheets[screen][] = css/forms.css
stylesheets[screen][] = css/html-elements.css
stylesheets[print][] = css/print.css

stylesheets[screen][] = css/origin.css


; Set the conditional stylesheets that are processed by IE.
conditional-stylesheets[if IE][all][] = css/ie.css


; Javascript files.
scripts[] = js/origin.js


; Theme settings.
features[] = logo
features[] = name
features[] = slogan
features[] = node_user_picture
features[] = comment_user_picture
features[] = favicon
features[] = main_menu
features[] = secondary_menu


; Regions.
regions[header] = Header
regions[help] = Help
regions[page_top] = Page top
regions[page_bottom] = Page bottom
regions[sidebar_first] = Sidebar first
regions[sidebar_second] = Sidebar second
regions[content] = Content
regions[footer] = Footer


